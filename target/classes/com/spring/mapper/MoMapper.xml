<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "com.spring.mapper.MemberMapper">
    
    <!-- 회원가입 -->
    <insert id="joinTry" parameterType="com.spring.mapper.MemberVO">
        insert into mo_member(id, pw, name, gender, age, checknum)
        values(#{id}, #{pw}, #{name}, #{gender}, #{age}, #{checknum})
    </insert>
    
 	<!-- 로그인 -->
    <select id="loginTry" parameterType="String" resultType="com.spring.mapper.MemberVO">
    	select * from mo_member where id = #{id}
    </select>
    
</mapper>